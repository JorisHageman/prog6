@model UserCreatedViewModel

@{
    ViewData["Title"] = "Gebruiker aangemaakt!";
}

<h1>@ViewData["Title"]</h1>

<div class="row">
    <div class="col-md-6">
        <p>Een nieuwe gebruiker is aangemaakt met de volgende gegevens:</p>
        <dl>
            <dt>Email</dt>
            <dd>@Model.Email</dd>
            <dt>Wachtwoord</dt>
            <dd>@Model.Password</dd>
        </dl>
    </div>
</div>